		{
			"name": "Auth",
			"item": [
				{
					"name": "Register",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const response = pm.response.json();",
									"",
									"pm.test(\"Should code is 201\", () => {",
									"    pm.response.to.have.status(201);",
									"});",
									"",
									"pm.test(\"Should return correct data\", () => {",
									"    pm.expect(response.id).to.be.an(\"string\");",
									"    pm.environment.set(\"user1_id\", response.id);",
									"",
									"    pm.expect(response.email).to.be.an(\"string\");",
									"    pm.environment.set(\"user1_email\", response.email);",
									"});",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"username\": \"usuario\",\n    \"password\": \"Ab@123Cd\",\n    \"email\": \"user@provider.com\"\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{url}}/user/register",
							"host": [
								"{{url}}"
							],
							"path": [
								"user",
								"register"
							]
						}
					},
					"response": []
				},
